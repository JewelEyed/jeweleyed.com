--- 
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="keywords" content="jeweleyed jewel obbie developer util about bio">
		<meta name="description" content="JewelEyed.com, my personal site.">
		<meta name="author" content="JewelEyed">
		<meta property="og:image" content="/favicon.png">
		<meta name="twitter:image" content="/favicon.png">
		<meta property="og:type" content="website">
		<meta property="og:site_name" content="JewelEyed.com">
		<meta property="og:title" content="JewelEyed's Website">
		<meta property="og:description" content="JewelEyed.com, my personal bio/link page.">
		<link rel="icon" type="image/png" href="/favicon.png" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>
